<p id="notice"><%= notice %></p>

<h1>Articale List</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articales.each do |articale| %>
      <tr>
        <td><%= articale.title %></td>
        <td><%= articale.content %></td>
        <td><%= link_to 'Show', articale, :id => articale.id %></td>
        <td><%= link_to 'Edit', edit_articale_path(articale) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Articale', new_articale_path %>
